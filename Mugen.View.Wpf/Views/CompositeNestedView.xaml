<UserControl x:Class="Mugen.Views.CompositeNestedView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Mugen.Views"
             xmlns:viewModels="clr-namespace:Mugen.ViewModels;assembly=Mugen.PortableCore"
             mc:Ignorable="d" d:DataContext="{d:DesignInstance viewModels:CompositeNestedViewModel}"
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <TextBlock Text="{Binding DisplayName}" />
    </Grid>
</UserControl>
